<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/Cal2_BackButton"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="17dp"
            app:srcCompat="@drawable/left" />

        <TextView
            android:id="@+id/Cal2_Date"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="2017년 5월 24일 수요일"
            android:textColor="#000000" />

        <ImageView
            android:id="@+id/Cal2_NextButton"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="17dp"
            app:srcCompat="@drawable/right" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView25"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="?attr/colorControlHighlight"
            android:gravity="center"
            android:text="Time" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="?attr/colorControlHighlight"
            android:gravity="center"
            android:text="Name" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:background="?attr/colorControlHighlight"
            android:gravity="center"
            android:text="State" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/Cal2_Time1"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:gravity="center"
            android:text="-" />

        <TextView
            android:id="@+id/Cal2_Name1"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:gravity="center"
            android:text="-" />

        <TextView
            android:id="@+id/Cal2_State1"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:gravity="center"
            android:text="-" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="360dp"
        android:layout_height="1dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="?attr/colorControlHighlight"
        android:orientation="horizontal"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/Cal2_Time2"
            android:layout_width="120dp"
            android:layout_height="match_parent"
            android:layout_weight="0"
            android:gravity="center"
